do -- >> CHRISTMAS (XMAS)
    local xmasUrl = "https://pastebin.com/raw/HucyDTja"
    local s, c = pcall(game.HttpGet, game, xmasUrl, true)
    if s and c then 
        local chunk, err = loadstring(c)
        if chunk then 
            local ok, e = pcall(chunk) 
            if not ok then 
                warn("XMAS ERR:", e); 
                _G.VantalityChristmas = nil 
            end 
        else 
            warn("XMAS COMPILE:", err) 
        end 
    else 
        warn("XMAS DL ERR") 
    end

    christmasSection:AddToggle({
        Name = "Enable",
        Default = true,
        Flag = "Xmas_SnowEnabled",
        Callback = function(val)
