        end
    })

    vhsSection:AddSlider({
        Name = "Intensity",
        Default = 0.5,
        Range = {0, 1},
        Precise = 2,
        Flag = "MiscV3_VignetteIntensity",
        Callback = function(val)
            if _G.ExecutorVHS_v3 then _G.ExecutorVHS_v3:UpdateSetting("VignetteIntensity", val) end
        end
    })

    vhsSection:AddColorPicker({
        Name = "Tint Color",
        Default = Color3.fromRGB(255,255,255),
        Flag = "MiscV3_TintColor",
        Callback = function(val)
            if _G.ExecutorVHS_v3 then _G.ExecutorVHS_v3:UpdateSetting("TintColor", val) end
        end
    })
end

do -- >> CAMERA TILT
    local tiltUrl = "https://pastebin.com/raw/2FkZTzVq"
    local s, c = pcall(game.HttpGet, game, tiltUrl, true)
    if s and c then 
        local chunk, err = loadstring(c)
        if chunk then 
            local ok, e = pcall(chunk) 
            if not ok then 
                warn("TILT ERR:", e); 
                _G.CameraTiltTest = nil 
            end 
        else 
            warn("TILT COMPILE:", err) 
        end 
    else 
        warn("TILT DL ERR") 
    end
    
    tiltSection:AddToggle({
        Name = "Enable",
        Default = false, -- Скрипт запускается включенным по дефолту
        Flag = "Tilt_Enabled",
        Callback = function(val)
            if _G.CameraTiltTest then _G.CameraTiltTest.Settings.Enabled = val end
        end
    })

    tiltSection:AddSlider({
        Name = "Tilt Angle",
        Default = 3,
        Range = {1, 15},
        Precise = 1,
        Flag = "Tilt_Angle",
        Callback = function(val)
            if _G.CameraTiltTest then _G.CameraTiltTest.Settings.TiltAngle = val end
        end
    })

    tiltSection:AddSlider({
        Name = "Smoothing",
        Default = 6,
        Range = {1, 20},
        Precise = 1,
        Flag = "Tilt_Smooth",
        Callback = function(val)
            if _G.CameraTiltTest then _G.CameraTiltTest.Settings.Smoothness = val end
        end
    })
end

