        Flag = "MiscV3_ColorCorrection",
        Callback = function(val)
            if _G.ExecutorVHS_v3 then _G.ExecutorVHS_v3:UpdateSetting("EnableColorCorrection", val) end
        end
    })

    vhsSection:AddSlider({
        Name = "Brightness",
        Default = 0,
        Range = {-0.5, 0.5},
        Precise = 2,
        Flag = "MiscV3_Brightness",
        Callback = function(val)
            if _G.ExecutorVHS_v3 then _G.ExecutorVHS_v3:UpdateSetting("Brightness", val) end
        end
    })

    vhsSection:AddSlider({
        Name = "Saturation",
        Default = 0,
        Range = {-0.5, 1},
        Precise = 2,
        Flag = "MiscV3_Saturation",
        Callback = function(val)
            if _G.ExecutorVHS_v3 then _G.ExecutorVHS_v3:UpdateSetting("Saturation", val) end
        end
    })

    vhsSection:AddSlider({
        Name = "Contrast",
