end

do -- >> MENU SETTINGS
    -- 1. Правильное определение устройства
    -- Если TouchEnabled = true, значит это телефон/планшет
    local UserInputService = game:GetService("UserInputService")
    local IsMobile = UserInputService.TouchEnabled 
    
    -- 2. Выбираем дефолтное значение для списка
    -- Если мобилка - "60", если ПК - "100"
    local StartScale = IsMobile and "60" or "100"
    
    settingsSection:AddDropdown({
        Name = "UI Theme",
        Values = ThemeMgr:GetThemeNames(),
        Default = "Fatality (Default)",
        Flag = "UI_Theme",
        Callback = function(val)
            ThemeMgr:Apply(Fatality, val)
        end
    })

    settingsSection:AddDropdown({
        Name = "Menu Size",
        Default = "100", 
        Values = {"60", "65", "75", "100", "125", "150"}, 
        Multi = false, 
        Flag = "UI_Scale",
        Callback = function(val)
        local scale = tonumber(val)
        if scale then
            Window:SetScale(scale)
        end
    end
})

    -- ПРИНУДИТЕЛЬНОЕ ПРИМЕНЕНИЕ ДЛЯ ТЕЛЕФОНОВ
    if IsMobile then
        Window:SetScale(60)
    end

    settingsSection:AddKeybind({
        Name = "Toggle Key",
        Default = Enum.KeyCode.Insert, 
        Flag = "UI_Keybind",
        OnlyPC = true,
        Callback = function(key)
            Window:SetKeybind(key) 
        end
    })
end

