        end
    })

    christmasSection:AddSlider({
        Name = "Snow Amount",
        Default = 800,
        Range = {10, 1500},
        Precise = 0,
        Flag = "Xmas_SnowAmount",
        Callback = function(val)
            if _G.VantalityChristmas then
                _G.VantalityChristmas.Settings.PARTICLE_COUNT = math.floor(val)
                _G.VantalityChristmas.RefreshParticles()
            end
        end
    })

    christmasSection:AddToggle({
        Name = "Enable Snowman",
        Default = true,
        Flag = "Xmas_SnowmanEnabled",
