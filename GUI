-- [MOBILE SUPPORT]
if not Fatality.IsPC then
    local MobileGui = Instance.new("ScreenGui")
    MobileGui.Name = "VantalityMobile"
    MobileGui.Parent = (gethui and gethui()) or game:GetService("CoreGui")
    _G.VantalityMobileGui = MobileGui
    
    local MobileButton = Window:GetButton()
    MobileButton.Parent = MobileGui
    MobileButton.Position = UDim2.new(0.5, -50, 0, 5)
    MobileButton.Size = UDim2.new(0, 100, 0, 35)
end
