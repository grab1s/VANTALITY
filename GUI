do -- >> SANTA HAT
    local hatUrl = "https://pastebin.com/raw/B5FY1ijk"
    local s, c = pcall(game.HttpGet, game, hatUrl, true)
    
    if s and c then 
        local chunk, err = loadstring(c)
        if chunk then 
            local ok, e = pcall(chunk) 
            if not ok then warn("SANTA HAT ERR:", e) end
        else 
            warn("SANTA HAT COMPILE:", err) 
        end 
    else 
        warn("SANTA HAT DL ERR") 
    end
    
    cosmeticsSection:AddToggle({
        Name = "Santa Hat",
        Default = false,
        Flag = "SantaHat_Enabled",
        Callback = function(val)
            if _G.SantaHatSettings then 
                _G.SantaHatSettings.Enabled = val
                -- Вызываем функцию обновления из скрипта (если она доступна глобально)
                if getgenv().UpdateSantaHatState then getgenv().UpdateSantaHatState() end
            end
        end
    })

    -- Настройка размера
    cosmeticsSection:AddSlider({
        Name = "Scale",
        Default = 1.1,
        Range = {0.5, 2.0},
        Precise = 2,
        Flag = "SantaHat_Scale",
        Callback = function(val)
            if _G.SantaHatSettings then
                _G.SantaHatSettings.Scale = Vector3.new(val, val, val)
            end
        end
    })

    -- Настройка высоты (Offset Y)
    cosmeticsSection:AddSlider({
        Name = "Height Y",
        Default = 0.2,
        Range = {-1.0, 1.5},
        Precise = 2,
        Flag = "SantaHat_OffsetY",
        Callback = function(val)
            if _G.SantaHatSettings then
                -- Сохраняем старые X и Z, меняем только Y
                local old = _G.SantaHatSettings.Offset
                _G.SantaHatSettings.Offset = Vector3.new(old.X, val, old.Z)
            end
        end
    })
end

