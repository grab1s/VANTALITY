-- [MOBILE CLEANUP]
if _G.VantalityMobileGui then
    if _G.VantalityMobileGui.Parent then _G.VantalityMobileGui:Destroy() end
    _G.VantalityMobileGui = nil
end

-- ПРОВЕРКА И ОЧИСТКА ОКРУЖЕНИЯ 
if _G.ahkgbs_instance then
    if type(_G.ahkgbs_instance) == "table" and _G.ahkgbs_instance.Main then
        _G.ahkgbs_instance.Main:Destroy()
    
    elseif typeof(_G.ahkgbs_instance) == "Instance" then
        _G.ahkgbs_instance:Destroy()
    end
    
    _G.ahkgbs_instance = nil
    
    if _G.VantalityChristmas and _G.VantalityChristmas.Maid then
        for _, v in pairs(_G.VantalityChristmas.Maid) do pcall(function() v:Destroy() end) end
        _G.VantalityChristmas = nil
    end
end

--  ЗАГРУЗКА БИБЛИОТЕКИ 
local Fatality_Success, Fatality_Result = pcall(function()
    return loadstring(game:HttpGet("https://raw.githubusercontent.com/grab1s/AHK-GBS/refs/heads/main/fatality.lib"))()
end)

if not Fatality_Success then
    warn("FATALITY: КРИТИЧЕСКАЯ ОШИБКА ЗАГРУЗКИ БИБЛИОТЕКИ:", Fatality_Result)
    return
end

if typeof(Fatality_Result) ~= "table" then
    warn("FATALITY: КРИТИЧЕСКАЯ ОШИБКА: Библиотека не является таблицей. Тип:", typeof(Fatality_Result))
    return
end

local Fatality = Fatality_Result

-- ИНИЦИАЛИЗАЦИЯ UI ОКНА
local Notification = Fatality:CreateNotifier()

task.spawn(function()
    local Sound = Instance.new("Sound")
    Sound.Name = "VantalityIntroFX"
    Sound.SoundId = "rbxassetid://138079767" 
    Sound.Volume = 4 
    Sound.Parent = game:GetService("SoundService") 
    Sound:Play()
    game:GetService("Debris"):AddItem(Sound, 20)
end)

local LoadingScreen = Fatality:Loader({
    Name = "Vantality",
    Scale = 3,
    Manual = true
})

Notification:Notify({
    Title = "Vantality",
    Content = "MERRY CHRISTMAS, " .. game.Players.LocalPlayer.DisplayName,
    Icon = "loader"
})

local Window = Fatality.new({
    Name = "Vantality",
    Expire = "NEVER!",
    Visible = false
})
_G.ahkgbs_instance = Window

local Config = Window:AddConfig()
Config:Init("Vantality", "TSB-Vantality-Config")

local ThemeSuccess, ThemeMgr = pcall(function()
    return loadstring(game:HttpGet("https://pastebin.com/raw/X0MN3W3X"))()
end)
if not ThemeSuccess or type(ThemeMgr) ~= "table" then
    warn("Themes failed to load!")
    ThemeMgr = { GetThemeNames = function() return {"Default"} end, Apply = function() end }
end


local cameraTab = Window:AddMenu({ Name = "CAMERA", Icon = "camera" })
local worldTab  = Window:AddMenu({ Name = "WORLD", Icon = "globe" })
local playerTab = Window:AddMenu({ Name = "PLAYER", Icon = "user" })
local flexTab   = Window:AddMenu({ Name = "FLEX (NO FE)", Icon = "crown" })
local miscTab   = Window:AddMenu({ Name = "MISC", Icon = "more-horizontal" })
local infoTab   = Window:AddMenu({ Name = "INFO", Icon = "info" })


-- CAMERA
local fovSection     = cameraTab:AddSection({ Position = 'left', Name = "FOV" })
local vhsSection     = cameraTab:AddSection({ Position = 'center', Name = "VHS" })
local tiltSection    = cameraTab:AddSection({ Position = 'right', Name = "CAMERA TILT" })
local stretchSection = cameraTab:AddSection({ Position = 'left', Name = "SCREEN STRETCH" })
local zoomSection    = cameraTab:AddSection({ Position = 'center', Name = "MAX ZOOM" })
local camBlurSection = cameraTab:AddSection({ Position = 'right', Name = "CAMERA BLUR" })

do -- >> FOV
    local fovScriptUrl = "https://pastebin.com/raw/kqpz3Ldg"
    local s, c = pcall(game.HttpGet, game, fovScriptUrl, true)
    if s and c then 
        local chunk, err = loadstring(c)
        if chunk then local ok,e = pcall(chunk) if not ok then warn("FOV ERR:", e); _G.FOV=nil end else warn("FOV COMPILE:", err) end 
    else 
        warn("FOV DL ERR") 
    end
    
    fovSection:AddToggle({
        Name = "Enable",
        Default = false,
        Flag = "Fov_Enabled",
        Callback = function(val)
            if not _G.FOV then return end
            if val then _G.FOV:enable() else _G.FOV:disable() end
        end
    })

    fovSection:AddSlider({
        Name = "FOV Value",
        Default = 90,
        Range = {60, 110},
        Precise = 0,
        Flag = "Fov_Value",
        Callback = function(val)
            if _G.FOV then _G.FOV:setBase(math.floor(val + 0.5)) end
        end
    })

    fovSection:AddToggle({
        Name = "Dynamic FOV",
        Default = false,
        Flag = "Fov_Dynamic",
        Callback = function(val)
            if _G.FOV then _G.FOV:setDynamicSpeedEnabled(val) end
        end
    })

    fovSection:AddSlider({
        Name = "FOV Boost",
        Default = 22,
        Range = {0, 38},
        Precise = 0,
        Flag = "Fov_Boost",
        Callback = function(val)
            if _G.FOV then _G.FOV:updateSetting("SpeedFovBoost", math.floor(val + 0.5)) end
        end
    })

    fovSection:AddSlider({
        Name = "Boost Speed",
        Default = 8,
        Range = {1, 25},
        Precise = 0,
        Flag = "Fov_BoostSpeed",
        Callback = function(val)
            if _G.FOV then _G.FOV:updateSetting("TransitionSpeed", math.floor(val + 0.5)) end
        end
    })
end

do -- >> VHS (Misc Effects)
    local miscUrl = "https://pastebin.com/raw/EHY0hztD"
    local s, c = pcall(game.HttpGet, game, miscUrl, true)
    if s and c then 
        local chunk, err = loadstring(c)
        if chunk then local ok,e = pcall(chunk) if not ok then warn("VHS ERR:", e); _G.ExecutorVHS_v3=nil end else warn("VHS COMPILE:", err) end 
    else 
        warn("VHS DL ERR") 
    end
    
