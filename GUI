-- PLAYER 
local arrowsSection      = playerTab:AddSection({ Position = 'left', Name = "PLAYER ARROWS" })
local NHespSection       = playerTab:AddSection({ Position = 'center', Name = "NAME / HEALTH (ESP)" })
local tracerSection      = playerTab:AddSection({ Position = 'right', Name = "VIEW TRACERS" })

do -- >> PLAYER ARROWS
